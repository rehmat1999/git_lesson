OR <-
function(rbar, a, b, v, lambda,pi_lag, I,o){
rbar + (a/b) + (v*(pi_lag - I + o))/(b*(lambda + v^2))
}
Yo <-
function(v,b,lambda,pibar,pi_lag,o){
v*(pibar - pi_lag -o)/(b*(lambda + v^2))
}
